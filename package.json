{
    "name": "graphql-bytes",
    "version": "1.0.0",
    "description": "Complimentary Git repo to the GraphQL Morning Bytes Presentation",
    "main": "src/index.js",
    "scripts": {
        "start": "node ./dist/index.js",
        "build": "./node_modules/.bin/babel src -d dist",
        "dev": "nodemon src --exec babel-node --config .nodemonrc.json",
        "clean": "rm -rf node_modules/ dist/ package-lock.json",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tameratgm/graphql-bytes.git"
    },
    "keywords": [
        "graphql",
        "education",
        "tutorial",
        "apollo-server"
    ],
    "author": "Tamer Aly",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/tameratgm/graphql-bytes/issues"
    },
    "homepage": "https://github.com/tameratgm/graphql-bytes#readme",
    "directories": {
        "test": "tests"
    },
    "devDependencies": {
        "@babel/cli": "7.10.5",
        "@babel/core": "7.11.4",
        "@babel/node": "7.10.5",
        "@babel/preset-env": "7.11.0",
        "jest": "26.4.2",
        "nodemon": "2.0.4"
    }
}
